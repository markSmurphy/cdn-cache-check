{
    "method": "head",
    "iterations": 1,
    "interval": 0,
    "headersCollection": "cloudfront",
    "headersCollections": [
        {
            "all": [
                "*"
            ]
        },
        {
            "cloudfront": [
                "x-cache",
                "age",
                "server",
                "x-amz-cf-pop",
                "cache-control"
            ]
        },
        {
            "fastly": [
                "x-cache",
                "x-cache-hits",
                "x-served-by",
                "age",
                "server",
                "fastly-*",
                "cache-control"
            ]
        },
        {
            "akamai": [
                "x-cache",
                "x-cache-key",
                "x-check-cacheable",
                "x-true-cache-key",
                "akamai-*",
                "x-akamai-*",
                "cache-control"
            ]
        },
        {
            "cache": [
                "x-cache",
                "age",
                "server",
                "x-amz-*",
                "cache-control",
                "expires",
                "vary"
            ]
        },
        {
            "CORS": [
                "access-control-*"
            ]
        },
        {
            "security": [
                "Content-Security-Policy",
                "Expect-CT",
                "Feature-Policy",
                "Public-Key-Pins",
                "Referrer-Policy",
                "Strict-Transport-Security",
                "X-Content-Type-Options",
                "X-Frame-Options",
                "X-Permitted-Cross-Domain-Policies",
                "X-XSS-Protection",
                "access-control-*"
            ]
        }
    ],
    "ApexDomains": {
        "*.cloudfront.net": "CloudFront",
        "*.edgekey.net": "Akamai",
        "*.edgesuite.net": "Akamai",
        "*.akamaiedge.net": "Akamai",
        "*.fastly.net": "Fastly"
    },
    "options" : {
        "timeout": 10000,
        "response_timeout": 0,
        "read_timeout": 0,
        "follow": 5,
        "export": true,
        "headers": {
            "user-agent": "ccc/{version} {OS}/{OSRelease}",
            "Connection": "close"
        }
    }
}